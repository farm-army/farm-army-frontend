<!doctype html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="{{ meta_description|default('Track your DeFi vaults on multiple chains') }}">
    <meta name="author" content="The yield farmer from the moon">
    <meta name="keywords" content="yield, bnb, binance, crypto, bitcoin, btc, defi, yield, yield farming, staking, pools, smart chain, polygon, matic, blockchain">

    <meta property="og:image" content="{{ asset('assets/img/farm_army_64.png') }}">
    <meta property="og:title" content="farm.army">
    <meta property="og:url" content="{{ absolute_url(path('frontpage')) }}">
    <meta property="og:description" content="Track your DeFi vaults on multiple chains with farm.army">

    {% block meta %}{% endblock meta %}

    <title>farm.army - {{ title }}</title>

    <!-- The page supports both dark and light color schemes,
          and the page author prefers / default is light. -->
    <meta name="color-scheme" content="light dark">

    <!-- Replace the Bootstrap CSS with the
         Bootstrap-Dark Variant CSS -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap-dark-5@1.1.3/dist/css/bootstrap-nightshade.min.css"
          rel="stylesheet">

    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.2/css/all.min.css"
          integrity="sha512-HK5fgLBL+xu6dm/Ii3z4xhlSUyZgTT9tuc/hSrtw6uzJOvgRr2a9jyxxT1ely+B+xFAmJKVSTbpM/CuL7qxO8w==" crossorigin="anonymous"/>

    {{ encore_entry_link_tags('main') }}

    {% block stylesheet %}
    {% endblock stylesheet %}

    <link rel="shortcut icon" href="{{ asset('favicon.ico') }}">
    <link rel="icon" type="image/png" href="{{ asset('assets/img/farm_army_32.png') }}" sizes="32x32">
    <link rel="apple-touch-icon" sizes="64x64" href="{{ asset('assets/img/farm_army_64.png') }}">

    <script src="https://cdn.jsdelivr.net/npm/bootstrap-dark-5@1.1.3/dist/js/darkmode.min.js"></script>
</head>

<body>

<header class="d-flex flex-column flex-md-row align-items-center mb-3 border-bottom shadow-sm">

<div class="container">
    <nav class="navbar navbar-expand-lg navbar-light">
        <div class="container-fluid">
            <a class="navbar-brand" href="{{ path('frontpage') }}">
                <img loading="lazy" src="{{ asset('assets/img/farm_army_32.png') }}" width="30" height="30" class="d-inline-block align-top" alt="farm.army"> farm.army
            </a>

            <a href="#" class="theme-toggle mx-2 px-3 fs-5 text-dark">
                <i class="fas fa-moon dark"></i>
                <i class="fas fa-lightbulb light"></i>
            </a>

            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbar-toggle" aria-controls="navbar-toggle" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>

            <div class="collapse navbar-collapse" id="navbar-toggle">
                <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                    <li class="nav-item">
                        <a class="nav-link" href="{{ path('frontpage') }}">Home</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="{{ path('arrivals') }}">Arrivals</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="{{ path('auto_farm') }}">Masterchef</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="{{ path('api_docu') }}">API</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" target="blank" href="https://twitter.com/FarmArmy">Twitter</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" target="blank" href="https://discord.gg/hzqAyDtepD">Discord</a>
                    </li>
                </ul>
                <div class="d-flex">
                    <a class="p-2 text-dark btn btn-outline-primary" href="{{ path('pools') }}"><i class="fas fa-fire text-danger"></i> Farming Pools</a>
                </div>
            </div>
        </div>
    </nav>
</div>
</header>

<main class="container">
    {% block content %}
    {% endblock content %}

    <footer class="pt-4 my-md-5 pt-md-5 border-top">
        <div class="row">
            <div class="col">
                <img loading="lazy" class="mb-2" src="{{ asset('assets/img/farm_army_32.png') }}" alt="" height="19">
                <small class="d-block mb-3 text-muted">farm.army - &copy; {{ 'now'|date('Y') }}</small>
            </div>
            <div class="col text-center">
                <h5>Contact</h5>
                <ul class="list-unstyled text-small">
                    <li><a class="link-secondary" href="https://twitter.com/FarmArmy">Twitter</a></li>
                    <li><a class="link-secondary" href="https://discord.gg/6myhn8HhWw">Discord</a></li>
                    <li><a class="link-secondary" href="https://github.com/farm-army">GitHub</a></li>
                </ul>
            </div>
            <div class="col text-end">
                <h5>Links</h5>
                <ul class="list-unstyled text-small">
                    <li><a class="link-secondary" target="_blank" href="https://www.binance.com/?ref=17569916">Binance</a></li>
                    <li><a class="link-secondary" target="_blank" href="https://www.coinbase.com/join/5a2ae60e76531100d3af2ee5">Coinbase</a></li>
                    <li><a class="link-secondary" target="_blank" href="https://www.bybit.com/app/register?ref=46AK7">Bybit</a></li>
                    <li><a class="link-secondary" target="_blank" href="https://ftx.com/#a=1494781">FTX</a></li>
                    <li><a class="link-secondary" target="_blank" href="https://www.bitfinex.com/?refcode=kDLceRHa">Bitfinex</a></li>
                    <li><a class="link-secondary" target="_blank" href="https://app.cakedefi.com?ref=053932">Cake DeFi</a></li>
                </ul>
            </div>
        </div>
    </footer>
</main>

<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
<script src="https://code.jquery.com/jquery-3.5.1.min.js" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>

{{ encore_entry_script_tags('main') }}

{% block javascript %}
{% endblock javascript %}

<script src="https://unpkg.com/@metamask/detect-provider/dist/detect-provider.min.js"></script>
<script src="https://www.w3counter.com/tracker.js?id=136783"></script>

</body>
</html>

